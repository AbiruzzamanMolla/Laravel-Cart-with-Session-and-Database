{
    "name": "azmolla/laravelcart",
    "description": "Laravel Shopping cart",
    "keywords": ["laravel", "shoppingcart", "cart"],
    "homepage": "https://github.com/AbiruzzamanMolla/Laravel-Cart-with-Session-and-Database",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Gloudemans",
            "email": "info@robgloudemans.nl"
        },
        {
            "name": "Michael V.",
            "email": "michael@michaelbelgium.me"
        },
        {
            "name": "Abiruzzaman Molla",
            "email": "abiruzzaman.molla@gmail.com",
            "homepage": "http://bornback.top/",
            "role": "Backend Developer"
        }
    ],
    "require": {
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/session": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/events": "^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.1",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^7"
    },
    "autoload": {
        "psr-4": {
            "Gloudemans\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gloudemans\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Gloudemans\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Gloudemans\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
